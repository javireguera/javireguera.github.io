&lt;p>You will be paying &amp;lsquo;Usecue&amp;rsquo; and you will see your order number as a reference. You will have to pay &lt;span id="paymenttotal">&lt;/span>.&lt;/p>
&lt;div id="smart-button-container" style="max-width: 350px;">

 &lt;input type="hidden" name="descriptionInput" id="description">
 &lt;input type="hidden" name="amountInput" id="amount">
 &lt;input type="hidden" name="invoiceid" id="invoiceid">

 &lt;script>
 window.onload = function() {
 var amount = (getCartTotal() + getAddonTotal()).toFixed(2); 
 var ordernumber = localStorage.getItem('ordernumber');
 
 document.getElementById('description').value = ordernumber;
 document.getElementById('invoiceid').value = ordernumber;
 document.getElementById('amount').value = amount;
 }
 &lt;/script>
 &lt;div style="text-align: center; margin-top: 0.625rem;" id="paypal-button-container">&lt;/div>
&lt;/div>

&lt;script src="https://www.paypal.com/sdk/js?client-id=ARLAOALKgJkpvAynfrxup8sP6NiLzBTpe6leWyLs7QJHnUMsCt8tKKVFWJzLg4zP0jky8IlPddUX2YCt&amp;currency=USD&amp;components=buttons">&lt;/script>
&lt;script>

function initPayPalButton() {

var amount = document.querySelector('#smart-button-container #amount');
var description = document.querySelector('#smart-button-container #description');
var invoiceid = document.querySelector('#smart-button-container #invoiceid');

var purchase_units = [];
purchase_units[0] = {};
purchase_units[0].amount = {};

function validate(event) {
 return event.value.length > 0;
}

paypal.Buttons({
 style: {
 color: 'blue',
 shape: 'pill',
 label: 'pay',
 layout: 'vertical',
 
 },

 onInit: function (data, actions) {
 
 },

 onClick: function () {
 
 purchase_units[0].description = description.value;
 purchase_units[0].amount.value = amount.value

 if(invoiceid.value !== '') {
 purchase_units[0].invoice_id = invoiceid.value;
 }
 },

 createOrder: function (data, actions) {
 return actions.order.create({
 purchase_units: purchase_units,
 });
 },

 onApprove: function (data, actions) {
 return actions.order.capture().then(function (details) {
 alert('Transaction completed by ' + details.payer.name.given_name + '!');
 });
 },

 onError: function (err) {
 console.log(err);
 }
}).render('#paypal-button-container');
}
initPayPalButton();
&lt;/script></description></item><item><title>prices</title><link>https://javireguera.github.io/pixelfoster/bricks/prices/</link><pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate><guid>https://javireguera.github.io/pixelfoster/bricks/prices/</guid><description>&lt;h2 id="our-friendly-pricing">Our friendly pricing&lt;/h2>
